<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title - Cuda Application</title>
        <link href="@Url.Content("~/Content/kendo/2014.1.318/kendo.common.min.css")" media="all" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/kendo/2014.1.318/kendo.default.min.css")" rel="stylesheet"/>
        
        <script src="@Url.Content("~/Scripts/modernizr-2.6.2.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery-2.1.0.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/kendo/2014.1.318/kendo.web.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/kendo.web.ext.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.center.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.blockUI.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.browser.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.cookie.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/kendo/culture/kendo.pt-br.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/kendo/languages/kendo.pt-br.js")" type="text/javascript"></script>

        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        
        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
    </head>
    <body>
        <header>
            <div class="content-wrapper">
                <div class="float-left">
                    <p class="site-title">@Html.ActionLink("Uma nova abordagem da programação paralela para GPU utilizando plataformas de desenvolvimento", "Index", "Home")</p>
                </div>
                <div class="float-left">
                    @*<section id="login">
                        @Html.Partial("_LoginPartial")
                    </section>*@
                    <nav>
                        <ul id="menu">
                            <li>@Html.ActionLink("O Projeto", "Index", "Home")</li>
                            <li>@Html.ActionLink("Nova Execução", "New", "Home")</li>
                            <li>@Html.ActionLink("Histórico", "History", "Home")</li>
                            <li>@Html.ActionLink("Autor", "About", "Home")</li>
                        </ul>
                    </nav>
                </div>
            </div>
        </header>
        <div id="body">
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
        <footer>
            <div class="content-wrapper">
                <div class="float-left">
                    <p>&copy; @DateTime.Now.Year - Paulo Figueiredo - Faculdade de Tecnologia - UNICAMP </p>
                </div>
            </div>
        </footer>

        @Scripts.Render("~/bundles/jquery")
        @RenderSection("scripts", required: false)
    </body>
</html>
